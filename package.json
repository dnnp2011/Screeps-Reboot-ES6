{
  "name": "reboot-es6",
  "version": "1.0.0",
  "description": "My ES6-enabled Screeps scripts",
  "main": "main.js",
  "scripts": {
    "purge-dist": "node -e \"require('fs-extra').emptyDir('./dist');\"",
    "purge": "rm -rf ./dist",
    "build": "rm -rf ./dist && babel src/** -d dist",
    "push-sim": "npm run build && node screeps.js sim",
    "push-main": "npm run build && node screeps.js main",
    "push": "npm run build && node screeps.js sim && node screeps.js main"
  },
  "keywords": [
    "screeps"
  ],
  "author": "Dalton Pierce",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "fs-extra": "^9.0.1",
    "git-rev-sync": "^2.0.0",
    "grunt": "^1.2.1",
    "grunt-screeps": "^1.4.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "lodash": "^4.17.19",
    "screeps-api": "^1.12.1"
  }
}
